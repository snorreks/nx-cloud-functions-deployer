{
	"root": "apps/functions",
	"sourceRoot": "apps/functions/src",
	"projectType": "application",
	"targets": {
		"deploy": {
			"executor": "../dist:deploy",
			"options": {
				"flavors": {
					"development": "mailvideo-test",
					"production": "example-production"
				},
				"node": "20",
				"minify": false
			}
		},
		"script": {
			"executor": "../dist:script",
			"options": {
				"flavors": {
					"development": "mailvideo-test",
					"production": "example-production"
				}
			}
		},
		"emulate": {
			"executor": "../dist:emulate",
			"options": {
				"flavors": {
					"development": "mailvideo-test",
					"production": "example-production"
				},
				"only": ["functions"],
				"packageManager": "global",
				"minify": false
			}
		}
	}
}
